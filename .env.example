# 環境設定ファイルのサンプル
# このファイルを.envにコピーして、実際の値を設定してください

ENV_FILE=.env.dev
DEBUG=True

# Django設定
DJANGO_SECRET_KEY=your-secret-key-here
DJANGO_HOST_PORT=7999

ALLOWED_HOSTS=localhost,127.0.0.1,web

# API設定
TSUKURUMA_API_HOST=your-api-host
TSUKURUMA_API_PORT=8000
APP_NAME=ai-sha-dev

# データベース設定
POSTGRES_DB=dev_db
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_pass
POSTGRES_HOST_PORT=5431

# コマンド設定
DJANGO_CMD=python manage.py runserver 0.0.0.0:8000

# React設定
REACT_BUILD_STAGE=dev
REACT_CMD=npm run dev
REACT_PORT=5173
NODE_ENV=development

# API URLs
AISHA_API_BASE=http://localhost:7999/api
MOCK_API_BASE=http://localhost:4000/api
MGDRIVE_API_BASE_URL=https://md2.mygare.jp/api

# Google Cloud Storage 設定
GCS_PROJECT_ID=your-project-id
GCS_BUCKET_NAME=your-bucket-name
GCS_CREDENTIALS_JSON=your-credentials-json
GOOGLE_APPLICATION_CREDENTIALS=/app/gcs-credentials.json

# Stripe 設定
STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_secret_here

# SUZURI API 設定
SUZURI_API_TOKEN=your-suzuri-api-token
SUZURI_API_BASE_URL=https://suzuri.jp/api/v1
SUZURI_DEMO_MODE=false

# AI API Keys
GEMINI_API_KEY=your-gemini-api-key
VITE_CLIPDROP_API_KEY=your-clipdrop-api-key
CLIPDROP_API_KEY=your-clipdrop-api-key

# Firebase設定
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-auth-domain
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id